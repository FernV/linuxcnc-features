			#<y-31> = [#<y-30> - #<cr>]
			#<x-32> = [#<x-40>]
			#<y-32> = [#<y-40> + #<cr>]
			#<x-41> = [#<x-40> + #<cr>]
			#<y-41> = [#<y-40>]
			#<x-42> = [#<x-10> - #<cr>]
			#<y-42> = [#<y-10>]
		o<dir-00> endif
		#<vx-12-21> = #<x-21>
		#<vy-12-21> = #<y-12>
		#<vx-22-31> = #<x-22>
		#<vy-22-31> = #<y-31>
		#<vx-32-41> = #<x-41>
		#<vy-32-41> = #<y-32>
		#<vx-42-11> = #<x-42>
		#<vy-42-11> = #<y-11>
	
