		o<l02> endif
	o<l01> endif

	#<_line:cos> = COS[#<_line:phi>]
	#<_line:sin> = SIN[#<_line:phi>]

	;(print line: len=#<_line:len> angle=#<_line:phi> cos=#<_line:cos> sin=#<_line:sin>)
