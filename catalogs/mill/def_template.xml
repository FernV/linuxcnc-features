<LinuxCNC-Features>
  <feature src="ini/fv_tool-change.ini" after="" help="&lt;b&gt;Add tool change gcode&lt;/b&gt;&#10;Changes setting accordingly even if disabled" o-name="Tool Change" image="tool-change.png" expanded="True" order="['PARAM_ENA', 'PARAM_H1', 'PARAM_N', 'PARAM_ACT', 'PARAM_TP', 'PARAM_COOLING', 'PARAM_H2', 'PARAM_H3', 'PARAM_SPEED', 'PARAM_F_SPEED', 'PARAM_H4', 'PARAM_FEED', 'PARAM_F_FEED', 'PARAM_V_FEED', 'PARAM_H5', 'PARAM_PEN', 'PARAM_STP', 'PARAM_PR', 'PARAM_PKR']" call="" selected="False" definitions="" before="&#10;(Tool change)&#10;o&lt;#self_id-ena&gt; if [#param_ena]&#10;&#9;M9&#10;&#9;T#param_n M6 G43&#10;&#9;#&lt;_tool_dia&gt; = #5410&#10;&#9;#&lt;_tool_d&gt; = #5410&#10;&#10;&#9;o&lt;#self_id-if&gt; if [#&lt;_tool_probe_set&gt;]&#10;&#9;&#9;o&lt;set-tool-length&gt; CALL&#10;&#9;o&lt;#self_id-if&gt; endif&#10;&#10;&#9;o&lt;#self_id-act&gt; if [#param_act EQ 0]&#10;&#9;&#9;M0&#10;&#9;o&lt;#self_id-act&gt; elseif [#param_act EQ 1]&#10;&#9;&#9;G4 P#param_tp&#10;&#9;&#9;M3&#10;&#9;o&lt;#self_id-act&gt; else (will auto-restart)&#10;&#9;&#9;M3&#10;&#9;o&lt;#self_id-act&gt; endif&#10;&#10;&#9;M#param_cooling&#10;o&lt;#self_id-ena&gt; endif&#10;&#10;S#param_speed&#10;F#param_feed&#10;&#10;#&lt;_final_feed&gt; = #param_f_feed&#10;#&lt;_final_spindle_rpm&gt; = #param_f_speed&#10;#&lt;_ffeed&gt; = #param_feed&#10;#&lt;_vertical_feed&gt; = #param_v_feed&#10;#&lt;_plunge_down&gt; = #param_pen&#10;#&lt;_depth_step&gt; = #param_stp&#10;#&lt;_plunge_ratio&gt; = #param_pr&#10;#&lt;_pocket_ratio&gt; = #param_pkr&#10;(End tool change)" path="0" icon="tool-change.png" type="toolchng" id="toolchng-002" name="Tool Change 002">
    <param tool_tip="Cutting params will be set even if disabled" name="Enabled" value="1" call="#param_ena" path="0:0" type="bool" options="Sans option"/>
    <param tool_tip="None" name="&lt;b&gt;--- General ---&lt;/b&gt;" call="#param_h1" path="0:1" type="header" options="Sans option"/>
    <param tool_tip="Tool number from table" name="Number" value="3" call="#param_n" path="0:2" icon="num.png" type="int" options="Sans option" minimum_value="0"/>
    <param tool_tip="After tool changed, it will..." name="Post action" value="2" call="#param_act" path="0:3" type="combo" options="Pause=0:Pause then restart=1:Auto restart=2"/>
    <param tool_tip="Timed pause of 'n' seconds to restart spindle" name="Pause for" value="5" call="#param_tp" path="0:4" type="int" options="Sans option" minimum_value="1"/>
    <param tool_tip="Use flood, mist or none" name="Use cooling" value="9" call="#param_cooling" path="0:5" type="combo" options="None=9:Flood=8:Mist=7" icon=""/>
    <param tool_tip="None" name="&lt;b&gt;--- Cutting Parameters ---&lt;/b&gt;" call="#param_h2" path="0:6" type="header" options="Sans option"/>
    <param tool_tip="None" name="&lt;b&gt;&lt;i&gt;  Speed...  &lt;/i&gt;&lt;/b&gt;" call="#param_h3" path="0:7" type="header" options="Sans option"/>
    <param tool_tip="Set spindle speed" name="Spindle speed" value="1000" call="#param_speed" path="0:8" icon="tool-spindle.png" type="int" options="Sans option" minimum_value="0"/>
    <param tool_tip="Spindle speed for final loop" name="Final spindle speed" value="1000" call="#param_f_speed" path="0:9" icon="tool-spindle.png" type="int" options="Sans option" minimum_value="0"/>
    <param tool_tip="None" name="&lt;b&gt;&lt;i&gt;  Feed...  &lt;/i&gt;&lt;/b&gt;" call="#param_h4" path="0:10" type="header" options="Sans option"/>
    <param tool_tip="Set XY feed" name="Feed" value="10.000" call="#param_feed" path="0:11" type="float" options="Sans option" icon="tool-feed.png"/>
    <param tool_tip="Feed for last loop" name="Final feed" value="10.000" call="#param_f_feed" path="0:12" type="float" options="Sans option" icon="tool-final-feed.png"/>
    <param tool_tip="Direct plunge feed when no XY move" name="Vertical feed" value="2.000" call="#param_v_feed" path="0:13" type="float" options="Sans option" icon="tool-vertical-feed.png"/>
    <param tool_tip="None" name="&lt;b&gt;&lt;i&gt;  Penetration...  &lt;/i&gt;&lt;/b&gt;" call="#param_h5" path="0:14" type="header" options="Sans option"/>
    <param tool_tip="How Z position is attained" name="Mode" value="0" call="#param_pen" path="0:15" type="combo" options="Plunge to Z while moving to XY=0:Plunge to Z then go to XY=1"/>
    <param tool_tip="Step down on each pass" name="Cut step" maximum_value="0.0" value="-0.100" call="#param_stp" path="0:16" type="float" options="Sans option" icon="depth.png"/>
    <param digits="2" tool_tip="Z versus XY move" name="Penetration ratio" maximum_value="1.0" value="0.07" call="#param_pr" path="0:17" minimum_value="0.0" type="float" options="Sans option" icon="tool-penetration-angle.png"/>
    <param digits="2" tool_tip="XY move versus tool diameter" name="Pocketing ratio" maximum_value="1.0" value="0.80" call="#param_pkr" path="0:18" minimum_value="0.0" type="float" options="Sans option" icon="depth.png"/>
    <param tool_tip="Subroutines that use this tool" expanded="True" name="Items" selected="False" call="#param_items" path="0:19" type="items" options="Sans option" icon="items.png">
      <feature tool_tip="&lt;b&gt;Creates a circle&lt;/b&gt;&#10;Can also create a D shape" src="ini/mill/fv_circle.ini" html_help="www.rds.ca" expanded="False" name="Circle 001" o-name="Circle" image="circle.png" after="" id="circle-001" call="&#10;&#9;o&lt;#self_id&gt; if [#param_ena]&#10;&#9;&#9;o&lt;fv_circle&gt; CALL [#param_cx][#param_cy][#param_d*#&lt;_units_r&gt;][#param_f*#&lt;_units_r&gt;] [#param_rot] [#param_opt] [#param_dir] [#param_s] [#param_dpt*#&lt;_units_d&gt;] [#param_fcut]&#10;&#9;o&lt;#self_id&gt; endif&#10;" selected="False" path="0:19:0" definitions="" icon="circle.png" type="circle" order="['PARAM_ENA', 'PARAM_FCUT', 'PARAM_H1', 'PARAM_CX', 'PARAM_CY', 'PARAM_D', 'PARAM_H2', 'PARAM_F', 'PARAM_ROT', 'PARAM_H3', 'PARAM_OPT', 'PARAM_DIR', 'PARAM_S', 'PARAM_DPT']" before="&#10;">
        <param tool_tip="None" name="Enabled" value="1" call="#param_ena" path="0:19:0:0" type="bool" options="No options"/>
        <param tool_tip="None" name="Show final cut" value="1" call="#param_fcut" path="0:19:0:1" type="bool" options="No options"/>
        <param tool_tip="None" name="&lt;b&gt;--- General ---&lt;/b&gt;" call="#param_h1" path="0:19:0:2" type="header" options="No options"/>
        <param tool_tip="Center of circle" name="cX" value="0.000" call="#param_cx" path="0:19:0:3" type="float" options="No options" icon="center.png"/>
        <param tool_tip="Center of circle" name="cY" value="0.000" call="#param_cy" path="0:19:0:4" type="float" options="No options" icon="center.png"/>
        <param tool_tip="Diameter" name="Diameter" value="1.500" call="#param_d" path="0:19:0:5" minimum_value="0.0" type="float" options="No options" icon="diameter.png"/>
        <param tool_tip="None" name="&lt;b&gt;--- Add a flat ---&lt;/b&gt;" call="#param_h2" path="0:19:0:6" type="header" options="No options"/>
        <param tool_tip="Height to remove from diameter" name="Remove" value="0.000" call="#param_f" path="0:19:0:7" icon="d-flat.png" type="float" options="No options" minimum_value="0.0"/>
        <param digits="2" tool_tip="Rotation of flat" name="Rotation" value="0.00" call="#param_rot" path="0:19:0:8" type="float" options="No options" icon="angle.png"/>
        <param tool_tip="None" name="&lt;b&gt;--- Cutting params ---&lt;/b&gt;" call="#param_h3" path="0:19:0:9" type="header" options="No options"/>
        <param tool_tip="Select tool path" name="Cut options" value="1" call="#param_opt" path="0:19:0:10" type="combo" options="Inside=0:Outside=1:On the line=2:Create a pocket=3" icon="g4x.png"/>
        <param tool_tip="Direction of path" name="Cut direction" value="2" call="#param_dir" path="0:19:0:11" type="combo" options="Clockwise=2:Counter-Clockwise=3" icon="cw-ccw.png"/>
        <param tool_tip="Surface is usual" name="Cut start" value="#&lt;_surface&gt;" call="#param_s" path="0:19:0:12" type="string" options="No options" icon="dz.png"/>
        <param tool_tip="Negative value" name="Cut down to" value="#&lt;_depth&gt;" call="#param_dpt" path="0:19:0:13" type="string" options="No options" icon="height.png"/>
      </feature>
    </param>
  </feature>
</LinuxCNC-Features>
