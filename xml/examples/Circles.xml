      <feature src="ini/fv_tool-change.ini" after="" help="&lt;b&gt;Add tool change gcode&lt;/b&gt;&#10;Changes setting accordingly even if disabled" o-name="Tool Change" image="tool-change.png" expanded="False" order="['PARAM_ENA', 'PARAM_H1', 'PARAM_N', 'PARAM_ACT', 'PARAM_TP', 'PARAM_COOLING', 'PARAM_H2', 'PARAM_H3', 'PARAM_SPEED', 'PARAM_F_SPEED', 'PARAM_H4', 'PARAM_FEED', 'PARAM_F_FEED', 'PARAM_V_FEED', 'PARAM_H5', 'PARAM_PEN', 'PARAM_STP', 'PARAM_PR', 'PARAM_PKR']" call="&#10;(Tool change)&#10;o&lt;#self_id-ena&gt; if [#param_ena]&#10;&#9;M9&#10;&#9;T#param_n M6 G43&#10;&#9;#&lt;_tool_dia&gt; = #5410&#10;&#9;#&lt;_tool_d&gt; = #5410&#10;&#10;&#9;o&lt;#self_id-if&gt; if [#&lt;_tool_probe_set&gt;]&#10;&#9;&#9;o&lt;set-tool-length&gt; CALL&#10;&#9;o&lt;#self_id-if&gt; endif&#10;&#10;&#9;o&lt;#self_id-act&gt; if [#param_act EQ 0]&#10;&#9;&#9;M0&#10;&#9;o&lt;#self_id-act&gt; elseif [#param_act EQ 1]&#10;&#9;&#9;G4 P#param_tp&#10;&#9;&#9;M3&#10;&#9;o&lt;#self_id-act&gt; else (will auto-restart)&#10;&#9;&#9;M3&#10;&#9;o&lt;#self_id-act&gt; endif&#10;&#10;&#9;M#param_cooling&#10;o&lt;#self_id-ena&gt; endif&#10;&#10;S#param_speed&#10;F#param_feed&#10;&#10;#&lt;_final_feed&gt; = #param_f_feed&#10;#&lt;_final_spindle_rpm&gt; = #param_f_speed&#10;#&lt;_ffeed&gt; = #param_feed&#10;#&lt;_vertical_feed&gt; = #param_v_feed&#10;#&lt;_plunge_down&gt; = #param_pen&#10;#&lt;_depth_step&gt; = #param_stp&#10;#&lt;_plunge_ratio&gt; = #param_pr&#10;#&lt;_pocket_ratio&gt; = #param_pkr&#10;(End tool change)" selected="False" definitions="" before="" path="0:10:1" icon="tool-change.png" type="toolchng" id="toolchng-002" name="Tool Change 002">
